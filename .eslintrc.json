{
  "env": {
    "browser": true,
    "es2022": true
  },
  "extends": [
    "airbnb",
    "airbnb-typescript/base",
    "eslint:recommended",
    "github/recommended",
    "unicorn/recommended",
    "@typescript-eslint/recommended",
    "solid/typescript"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "solid", "github", "unicorn"],
  "rules": {
    "quotes": ["error", "double"],
    "semi": ["error", "always"]
  }
}
